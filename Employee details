CREATE DATABASE company;
CREATE TABLE Employee(
EmployeeID int primary key,
EmployeeName varchar(25) not null,
ContactName varchar(25),
PhoneNumber int unique,
city varchar(25)
); 
INSERT INTO Employee VALUES(1,"ASHWINI","ASHU",74,"MANDYA");
